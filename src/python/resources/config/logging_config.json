{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "normal": {
            "format": "%(asctime)s [%(levelname)-5s] %(name)s - %(message)s"
        },
        "detailed": {
            "format": "%(asctime)s | %(levelname)-5s | %(name)s | %(funcName)s | %(lineno)s | %(message)s"
        }
    },
    "handlers": {
    },
    "loggers": {
        "application": {
            "level": "DEBUG"
        },
        "database": {
            "level": "DEBUG"
        },
        "algorithm": {
            "level": "DEBUG"
        },
        "server": {
            "level": "DEBUG"
        }
    }
}